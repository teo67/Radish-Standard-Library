d Tests p import "../../Tests"
h t() {
    d str p "hello, world!"
    h new Tests(["length", "indexOf", "substring", "begins", "ends", "has", "split"], [
        t() {
            h str.length == 13
        }, t() {
            h str.indexOf("l") == 2
        }, t() {
            h str.substring(4, 8) == "o, w"
        }, t() {
            h str.begins("hello")
        }, t() {
            h str.ends("d!")
        }, t() {
            h str.has("w")
        }, t() {
            h str.split()[0] == "h"
        }
    ]).execute()
}